---
import Layout from '../layouts/Layout.astro';
import TransactionForm from '../components/TransactionForm';
import DataTable from '../components/DataTable';
---

<Layout title="Investment dashboard">
	<main class="m-5 grid grid-cols-2 grid-rows-1 gap-5">
    <div class="shadow-md p-5 h-fit">
      <TransactionForm client:load></TransactionForm>
    </div>
    <div class="shadow-md p-5">
      <DataTable client:load></DataTable>
    </div>
	</main>
</Layout>
